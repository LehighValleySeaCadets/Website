{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/assets/*" ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/",
      "redirect": "/index.html",
      "statusCode": 301
    }
  ]
}